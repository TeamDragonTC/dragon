<launch>
  <node pkg="joy_linux" exec="joy_linux_node" name="joy_linux_node"/>

  <node pkg="dragon_planner" exec="cugo_teleop_twist_joy" name="cugo_teleop_twist_joy">
    <remap from="twist" to="cmd_vel_raw"/>
  </node>

  <node pkg="dragon_planner exec="cugo_teleop" name="cugo_teleop_node" output="screen">
    <param name="port_name" value="/dev/ttyACM0"/>
  </node>

</launch>
